{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: 'DischargeView',\n  data() {\n    return {\n      startDate: '',\n      endDate: '',\n      searchKeyword: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      showPrintModal: false,\n      selectedPatient: {},\n      diagnoses: ['高血压病', '美尼尔综合征', '老年性精神障碍', '阻塞钙石床后'],\n      patients: [{\n        id: 1,\n        name: '李大哥',\n        idNumber: '310106194805162428',\n        gender: '男',\n        age: 78,\n        admissionDate: '2024-05-22',\n        dischargeReason: '自动出院',\n        dischargeDate: '2025-01-22',\n        dischargeSummary: {\n          status: 'pending',\n          time: '',\n          operator: ''\n        },\n        dischargeHanding: {\n          status: 'completed',\n          time: '',\n          operator: '王主任'\n        }\n      }, {\n        id: 2,\n        name: '王大娘',\n        idNumber: '310106194805162428',\n        gender: '女',\n        age: 98,\n        admissionDate: '2023-05-18',\n        dischargeReason: '本院死亡',\n        dischargeDate: '2024-01-18',\n        dischargeSummary: {\n          status: 'completed',\n          time: '2024-05-18 12:18',\n          operator: '刘医生'\n        },\n        dischargeHanding: {\n          status: 'completed',\n          time: '',\n          operator: '王主任'\n        }\n      }, {\n        id: 3,\n        name: '宋老博',\n        idNumber: '310106194805162428',\n        gender: '男',\n        age: 89,\n        admissionDate: '2024-05-12',\n        dischargeReason: '外院死亡',\n        dischargeDate: '2024-09-12',\n        dischargeSummary: {\n          status: 'completed',\n          time: '2024-05-18 12:18',\n          operator: '刘医生'\n        },\n        dischargeHanding: {\n          status: 'completed',\n          time: '2024-05-18 12:18',\n          operator: '张护士'\n        }\n      }]\n    };\n  },\n  computed: {\n    filteredPatients() {\n      let result = [...this.patients];\n      if (this.searchKeyword) {\n        const keyword = this.searchKeyword.toLowerCase();\n        result = result.filter(patient => patient.name.toLowerCase().includes(keyword) || patient.idNumber.includes(keyword));\n      }\n      if (this.startDate && this.endDate) {\n        const start = new Date(this.startDate);\n        const end = new Date(this.endDate);\n        result = result.filter(patient => {\n          const dischargeDate = new Date(patient.dischargeDate);\n          return dischargeDate >= start && dischargeDate <= end;\n        });\n      }\n      return result;\n    }\n  },\n  methods: {\n    searchPatients() {\n      this.currentPage = 1;\n    },\n    handleSizeChange(val) {\n      this.pageSize = val;\n      this.currentPage = 1;\n    },\n    handleCurrentChange(val) {\n      this.currentPage = val;\n    },\n    editDischarge(patient) {\n      this.$router.push(`/discharge-form/${patient.id}`);\n    },\n    printDischarge(patient) {\n      this.selectedPatient = patient;\n      this.showPrintModal = true;\n    },\n    viewDischargeDetails(patient) {\n      this.$router.push({\n        path: `/discharge-form/${patient.id}`,\n        query: {\n          mode: 'view'\n        }\n      });\n    },\n    canEdit(patient) {\n      return patient.id !== 3;\n    },\n    closePrintModal() {\n      this.showPrintModal = false;\n    },\n    actualPrint() {\n      window.print();\n      this.closePrintModal();\n    },\n    getStatusType(status) {\n      return status === 'pending' ? 'warning' : 'success';\n    }\n  }\n};","map":{"version":3,"names":["name","data","startDate","endDate","searchKeyword","currentPage","pageSize","total","showPrintModal","selectedPatient","diagnoses","patients","id","idNumber","gender","age","admissionDate","dischargeReason","dischargeDate","dischargeSummary","status","time","operator","dischargeHanding","computed","filteredPatients","result","keyword","toLowerCase","filter","patient","includes","start","Date","end","methods","searchPatients","handleSizeChange","val","handleCurrentChange","editDischarge","$router","push","printDischarge","viewDischargeDetails","path","query","mode","canEdit","closePrintModal","actualPrint","window","print","getStatusType"],"sources":["src/views/Discharge.vue"],"sourcesContent":["<template>\r\n  <div class=\"assessment-detail-container\">\r\n    <!-- 标题卡片 -->\r\n    <el-card class=\"info-card\">\r\n      <div slot=\"header\" class=\"card-header\">\r\n        <el-icon><UserFilled /></el-icon>\r\n        <span>离院办理</span>\r\n      </div>\r\n    </el-card>\r\n\r\n    <!-- 搜索和筛选区域 -->\r\n    <div class=\"assessment-content\">\r\n      <div class=\"assessment-section\">\r\n        <div class=\"section-content\">\r\n          <div class=\"filter-area\">\r\n            <div class=\"date-filter\">\r\n              <span class=\"filter-label\">离院日期</span>\r\n              <el-date-picker\r\n                v-model=\"startDate\"\r\n                type=\"date\"\r\n                placeholder=\"开始日期\"\r\n                format=\"yyyy/MM/dd\"\r\n                value-format=\"yyyy-MM-dd\">\r\n              </el-date-picker>\r\n              <span>至</span>\r\n              <el-date-picker\r\n                v-model=\"endDate\"\r\n                type=\"date\"\r\n                placeholder=\"结束日期\"\r\n                format=\"yyyy/MM/dd\"\r\n                value-format=\"yyyy-MM-dd\">\r\n              </el-date-picker>\r\n            </div>\r\n            <div class=\"search-box\">\r\n              <el-input\r\n                v-model=\"searchKeyword\"\r\n                placeholder=\"姓名/入院编号\"\r\n                class=\"search-input\">\r\n                <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"searchPatients\"></el-button>\r\n              </el-input>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 提示信息 -->\r\n          <div class=\"notice\">\r\n            <p>提示：完成离院流程手续后，离院小结数据不可再修改</p>\r\n          </div>\r\n\r\n          <!-- 表格内容 -->\r\n          <el-table\r\n            :data=\"filteredPatients\"\r\n            style=\"width: 100%\"\r\n            border>\r\n            <el-table-column\r\n              label=\"姓名/身份证\"\r\n              min-width=\"180\">\r\n              <template slot-scope=\"scope\">\r\n                <div>{{ scope.row.name }}</div>\r\n                <div class=\"id-number\">{{ scope.row.idNumber }}</div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"gender\"\r\n              label=\"性别\"\r\n              width=\"80\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"age\"\r\n              label=\"年龄\"\r\n              width=\"80\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"admissionDate\"\r\n              label=\"入院日期\"\r\n              width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"dischargeReason\"\r\n              label=\"离院原因\"\r\n              width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"dischargeDate\"\r\n              label=\"离院日期\"\r\n              width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              label=\"离院小结\"\r\n              width=\"120\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag :type=\"getStatusType(scope.row.dischargeSummary.status)\">\r\n                  {{ scope.row.dischargeSummary.status === 'pending' ? '待录入' : '已完成' }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column\r\n              label=\"离院交接\"\r\n              width=\"120\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag :type=\"getStatusType(scope.row.dischargeHanding.status)\">\r\n                  {{ scope.row.dischargeHanding.status === 'pending' ? '待交接' : '已完成' }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column\r\n              label=\"操作\"\r\n              width=\"120\"\r\n              fixed=\"right\">\r\n              <template slot-scope=\"scope\">\r\n                <div class=\"action-btns\">\r\n                  <el-button\r\n                    v-if=\"scope.row.dischargeSummary.status === 'pending'\"\r\n                    type=\"text\"\r\n                    size=\"small\"\r\n                    @click=\"editDischarge(scope.row)\">\r\n                    <i class=\"el-icon-edit\"></i>\r\n                  </el-button>\r\n                  <el-button\r\n                    v-else\r\n                    type=\"text\"\r\n                    size=\"small\"\r\n                    @click=\"printDischarge(scope.row)\">\r\n                    <i class=\"el-icon-printer\"></i>\r\n                  </el-button>\r\n                  <el-button\r\n                    v-if=\"canEdit(scope.row)\"\r\n                    type=\"text\"\r\n                    size=\"small\"\r\n                    @click=\"editDischarge(scope.row)\">\r\n                    <i class=\"el-icon-edit\"></i>\r\n                  </el-button>\r\n                  <el-button\r\n                    v-else\r\n                    type=\"text\"\r\n                    size=\"small\"\r\n                    @click=\"viewDischargeDetails(scope.row)\">\r\n                    <i class=\"el-icon-view\"></i>\r\n                  </el-button>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n\r\n          <!-- 分页 -->\r\n          <div class=\"pagination-section\">\r\n            <el-pagination\r\n              @size-change=\"handleSizeChange\"\r\n              @current-change=\"handleCurrentChange\"\r\n              :current-page=\"currentPage\"\r\n              :page-sizes=\"[10, 20, 50]\"\r\n              :page-size=\"pageSize\"\r\n              layout=\"total, sizes, prev, pager, next, jumper\"\r\n              :total=\"total\">\r\n            </el-pagination>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 打印模态框 -->\r\n    <el-dialog\r\n      title=\"出院小结\"\r\n      :visible.sync=\"showPrintModal\"\r\n      width=\"800px\"\r\n      class=\"print-dialog\">\r\n      <div class=\"print-modal-content\">\r\n        <div class=\"print-header\">\r\n          <h2>上海XX机构</h2>\r\n          <h3>出院小结</h3>\r\n        </div>\r\n\r\n        <div class=\"section-content\">\r\n          <el-form label-width=\"100px\" class=\"patient-info\">\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"8\">\r\n                <el-form-item label=\"姓名：\">\r\n                  <span>{{ selectedPatient.name }}</span>\r\n                </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <el-form-item label=\"性别：\">\r\n                  <span>{{ selectedPatient.gender }}</span>\r\n                </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <el-form-item label=\"年龄：\">\r\n                  <span>{{ selectedPatient.age }}</span>\r\n                </el-form-item>\r\n              </el-col>\r\n            </el-row>\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"入院日期：\">\r\n                  <span>{{ selectedPatient.admissionDate }}</span>\r\n                </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <el-form-item label=\"出院日期：\">\r\n                  <span>{{ selectedPatient.dischargeDate }}</span>\r\n                </el-form-item>\r\n              </el-col>\r\n            </el-row>\r\n          </el-form>\r\n\r\n          <div class=\"assessment-section\">\r\n            <h3 class=\"section-title\">入院诊断</h3>\r\n            <div class=\"diagnosis-list\">\r\n              <div v-for=\"(diagnosis, index) in diagnoses\" :key=\"index\">\r\n                {{ index + 1 }}. {{ diagnosis }}\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"assessment-section\">\r\n            <h3 class=\"section-title\">入院情况</h3>\r\n            <div class=\"treatment-content\">\r\n              {{ selectedPatient.admissionCondition }}\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"assessment-section\">\r\n            <h3 class=\"section-title\">在院治疗情况</h3>\r\n            <div class=\"treatment-content\">\r\n              {{ selectedPatient.treatmentProcess }}\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"assessment-section\">\r\n            <h3 class=\"section-title\">出院情况</h3>\r\n            <div class=\"treatment-content\">\r\n              {{ selectedPatient.dischargeCondition }}\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"closePrintModal\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"actualPrint\">打 印</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'DischargeView',\r\n  data() {\r\n    return {\r\n      startDate: '',\r\n      endDate: '',\r\n      searchKeyword: '',\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      showPrintModal: false,\r\n      selectedPatient: {},\r\n      diagnoses: [\r\n        '高血压病',\r\n        '美尼尔综合征',\r\n        '老年性精神障碍',\r\n        '阻塞钙石床后'\r\n      ],\r\n      patients: [\r\n        {\r\n          id: 1,\r\n          name: '李大哥',\r\n          idNumber: '310106194805162428',\r\n          gender: '男',\r\n          age: 78,\r\n          admissionDate: '2024-05-22',\r\n          dischargeReason: '自动出院',\r\n          dischargeDate: '2025-01-22',\r\n          dischargeSummary: { status: 'pending', time: '', operator: '' },\r\n          dischargeHanding: { status: 'completed', time: '', operator: '王主任' }\r\n        },\r\n        {\r\n          id: 2,\r\n          name: '王大娘',\r\n          idNumber: '310106194805162428',\r\n          gender: '女',\r\n          age: 98,\r\n          admissionDate: '2023-05-18',\r\n          dischargeReason: '本院死亡',\r\n          dischargeDate: '2024-01-18',\r\n          dischargeSummary: { status: 'completed', time: '2024-05-18 12:18', operator: '刘医生' },\r\n          dischargeHanding: { status: 'completed', time: '', operator: '王主任' }\r\n        },\r\n        {\r\n          id: 3,\r\n          name: '宋老博',\r\n          idNumber: '310106194805162428',\r\n          gender: '男',\r\n          age: 89,\r\n          admissionDate: '2024-05-12',\r\n          dischargeReason: '外院死亡',\r\n          dischargeDate: '2024-09-12',\r\n          dischargeSummary: { status: 'completed', time: '2024-05-18 12:18', operator: '刘医生' },\r\n          dischargeHanding: { status: 'completed', time: '2024-05-18 12:18', operator: '张护士' }\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  computed: {\r\n    filteredPatients() {\r\n      let result = [...this.patients];\r\n      \r\n      if (this.searchKeyword) {\r\n        const keyword = this.searchKeyword.toLowerCase();\r\n        result = result.filter(patient => \r\n          patient.name.toLowerCase().includes(keyword) ||\r\n          patient.idNumber.includes(keyword)\r\n        );\r\n      }\r\n      \r\n      if (this.startDate && this.endDate) {\r\n        const start = new Date(this.startDate);\r\n        const end = new Date(this.endDate);\r\n        result = result.filter(patient => {\r\n          const dischargeDate = new Date(patient.dischargeDate);\r\n          return dischargeDate >= start && dischargeDate <= end;\r\n        });\r\n      }\r\n      \r\n      return result;\r\n    }\r\n  },\r\n  methods: {\r\n    searchPatients() {\r\n      this.currentPage = 1;\r\n    },\r\n    handleSizeChange(val) {\r\n      this.pageSize = val;\r\n      this.currentPage = 1;\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val;\r\n    },\r\n    editDischarge(patient) {\r\n      this.$router.push(`/discharge-form/${patient.id}`);\r\n    },\r\n    printDischarge(patient) {\r\n      this.selectedPatient = patient;\r\n      this.showPrintModal = true;\r\n    },\r\n    viewDischargeDetails(patient) {\r\n      this.$router.push({\r\n        path: `/discharge-form/${patient.id}`,\r\n        query: { mode: 'view' }\r\n      });\r\n    },\r\n    canEdit(patient) {\r\n      return patient.id !== 3;\r\n    },\r\n    closePrintModal() {\r\n      this.showPrintModal = false;\r\n    },\r\n    actualPrint() {\r\n      window.print();\r\n      this.closePrintModal();\r\n    },\r\n    getStatusType(status) {\r\n      return status === 'pending' ? 'warning' : 'success';\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n/* 使用HealthAssessment.vue的样式变量 */\r\n:root {\r\n  --morandiBlue: #94b0b8;\r\n  --morandiGreen: #8ba89f;\r\n  --morandiTeal: #7da4a4;\r\n  --morandiGray: #b4bcc0;\r\n  --morandiLightBlue: #c5d5da;\r\n  --morandiLightGreen: #cad7d3;\r\n  --morandiBg: #f5f7f8;\r\n  --morandiText: #5c6b73;\r\n  --morandiDark: #445963;\r\n  --morandiHover: #86a5ae;\r\n}\r\n\r\n.assessment-detail-container {\r\n  padding: 24px;\r\n  background-color: var(--morandiBg);\r\n  min-height: 100vh;\r\n}\r\n\r\n.info-card {\r\n  margin-bottom: 24px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 12px 0 rgba(148, 176, 184, 0.1);\r\n  transition: all 0.3s ease;\r\n  background-color: #fff;\r\n  \r\n  &:hover {\r\n    transform: translateY(-2px);\r\n    box-shadow: 0 4px 16px 0 rgba(148, 176, 184, 0.15);\r\n  }\r\n  \r\n  .card-header {\r\n    display: flex;\r\n    align-items: center;\r\n    padding: 16px 20px;\r\n    border-bottom: 1px solid var(--morandiLightBlue);\r\n    \r\n    .el-icon {\r\n      margin-right: 12px;\r\n      font-size: 20px;\r\n      color: var(--morandiTeal);\r\n    }\r\n\r\n    span {\r\n      font-size: 16px;\r\n      font-weight: 600;\r\n      color: var(--morandiDark);\r\n    }\r\n  }\r\n}\r\n\r\n.assessment-content {\r\n  background-color: #fff;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.05);\r\n  padding: 24px;\r\n  margin-bottom: 24px;\r\n}\r\n\r\n.assessment-section {\r\n  margin-bottom: 32px;\r\n  \r\n  &:last-child {\r\n    margin-bottom: 0;\r\n  }\r\n\r\n  .section-title {\r\n    font-size: 16px;\r\n    font-weight: 600;\r\n    color: var(--morandiDark);\r\n    margin-bottom: 20px;\r\n    position: relative;\r\n    padding-left: 12px;\r\n\r\n    &:before {\r\n      content: '';\r\n      position: absolute;\r\n      left: 0;\r\n      top: 50%;\r\n      transform: translateY(-50%);\r\n      width: 4px;\r\n      height: 16px;\r\n      background-color: var(--morandiTeal);\r\n      border-radius: 2px;\r\n    }\r\n  }\r\n}\r\n\r\n.filter-area {\r\n  margin-bottom: 16px;\r\n  background: #e8ecef;\r\n  padding: 16px;\r\n  border-radius: 6px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  gap: 16px;\r\n\r\n  .date-filter {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 8px;\r\n\r\n    .filter-label {\r\n      color: var(--morandiText);\r\n      font-weight: 500;\r\n    }\r\n  }\r\n\r\n  .search-box {\r\n    width: 280px;\r\n\r\n    .search-input {\r\n      width: 100%;\r\n    }\r\n  }\r\n}\r\n\r\n.notice {\r\n  padding: 12px 16px;\r\n  background: rgba(58, 125, 124, 0.1);\r\n  border-radius: 4px;\r\n  margin-bottom: 20px;\r\n\r\n  p {\r\n    color: var(--morandiTeal);\r\n    font-size: 13px;\r\n    margin: 0;\r\n  }\r\n}\r\n\r\n/* 表格样式 */\r\n.el-table {\r\n  margin-top: 16px;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n\r\n  ::v-deep {\r\n    th {\r\n      background-color: #f5f7f8;\r\n      color: var(--morandiDark);\r\n      font-weight: 600;\r\n    }\r\n\r\n    .el-table__row {\r\n      &:hover > td {\r\n        background-color: var(--morandiBg);\r\n      }\r\n    }\r\n  }\r\n\r\n  .id-number {\r\n    font-size: 12px;\r\n    color: var(--morandiText);\r\n    margin-top: 4px;\r\n  }\r\n}\r\n\r\n/* 标签样式 */\r\n.el-tag {\r\n  border-radius: 4px;\r\n  padding: 0 8px;\r\n  height: 24px;\r\n  line-height: 22px;\r\n\r\n  &.el-tag--warning {\r\n    background-color: rgba(230, 162, 60, 0.1);\r\n    border-color: rgba(230, 162, 60, 0.2);\r\n    color: #e6a23c;\r\n  }\r\n\r\n  &.el-tag--success {\r\n    background-color: rgba(103, 194, 58, 0.1);\r\n    border-color: rgba(103, 194, 58, 0.2);\r\n    color: #67c23a;\r\n  }\r\n}\r\n\r\n/* 操作按钮样式 */\r\n.action-btns {\r\n  display: flex;\r\n  justify-content: center;\r\n  gap: 8px;\r\n\r\n  .el-button {\r\n    padding: 0;\r\n    font-size: 16px;\r\n    \r\n    &:hover {\r\n      color: var(--morandiTeal);\r\n    }\r\n  }\r\n}\r\n\r\n/* 分页样式 */\r\n.pagination-section {\r\n  margin-top: 24px;\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  padding-top: 16px;\r\n  border-top: 1px solid #f0f2f5;\r\n}\r\n\r\n/* 打印弹窗样式 */\r\n.print-dialog {\r\n  ::v-deep .el-dialog__body {\r\n    padding: 24px;\r\n  }\r\n}\r\n\r\n.print-modal-content {\r\n  .print-header {\r\n    text-align: center;\r\n    margin-bottom: 24px;\r\n    \r\n    h2 {\r\n      font-size: 20px;\r\n      color: var(--morandiDark);\r\n      margin-bottom: 8px;\r\n    }\r\n    \r\n    h3 {\r\n      font-size: 16px;\r\n      color: var(--morandiText);\r\n      font-weight: normal;\r\n    }\r\n  }\r\n\r\n  .diagnosis-list {\r\n    padding: 0 16px;\r\n    \r\n    div {\r\n      margin-bottom: 8px;\r\n      color: var(--morandiText);\r\n    }\r\n  }\r\n\r\n  .treatment-content {\r\n    padding: 0 16px;\r\n    color: var(--morandiText);\r\n    line-height: 1.6;\r\n  }\r\n}\r\n\r\n.dialog-footer {\r\n  text-align: ri