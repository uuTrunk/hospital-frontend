{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { getDischargeList } from '../api/discharge';\nexport default {\n  name: 'DischargeManagement',\n  data() {\n    return {\n      loading: false,\n      tableData: [],\n      total: 0,\n      currentPage: 1,\n      pageSize: 10,\n      dateRange: [],\n      searchQuery: '',\n      activeTab: 'discharge',\n      showPrintModal: false,\n      selectedPatient: {}\n    };\n  },\n  methods: {\n    async fetchData() {\n      this.loading = true;\n      try {\n        const params = {\n          page: this.currentPage - 1,\n          size: this.pageSize\n        };\n        if (this.dateRange && this.dateRange.length === 2) {\n          params.startDate = this.dateRange[0];\n          params.endDate = this.dateRange[1];\n        }\n        if (this.searchQuery) {\n          params.nameOrCode = this.searchQuery;\n        }\n        const response = await getDischargeList(params);\n        if (response.code === 200) {\n          this.tableData = response.data.content;\n          this.total = response.data.totalElements;\n        } else {\n          this.$message.error(response.message || '获取数据失败');\n        }\n      } catch (error) {\n        console.error('获取数据失败:', error);\n        this.$message.error('获取数据失败');\n      } finally {\n        this.loading = false;\n      }\n    },\n    handleDateChange() {\n      this.currentPage = 1;\n      this.fetchData();\n    },\n    handleSearch() {\n      this.currentPage = 1;\n      this.fetchData();\n    },\n    handleReset() {\n      this.dateRange = [];\n      this.searchQuery = '';\n      this.currentPage = 1;\n      this.fetchData();\n    },\n    handleSizeChange(val) {\n      this.pageSize = val;\n      this.fetchData();\n    },\n    handleCurrentChange(val) {\n      this.currentPage = val;\n      this.fetchData();\n    },\n    goToAssessment() {\n      this.$router.push('/ass-assessment');\n    },\n    getStatusType(status) {\n      return status === 'pending' ? 'warning' : 'success';\n    },\n    getOperationButton(row) {\n      if (row.dischargeSummary.status === 'completed' && row.dischargeHanding.status === 'completed') {\n        return {\n          type: 'view',\n          icon: 'el-icon-view',\n          text: '查看'\n        };\n      } else {\n        return {\n          type: 'edit',\n          icon: 'el-icon-edit',\n          text: '编辑'\n        };\n      }\n    },\n    editDischarge(row) {\n      this.$router.push({\n        path: '/discharge-form',\n        query: {\n          id: row.id\n        }\n      });\n    },\n    viewDischargeDetails(row) {\n      this.$router.push({\n        path: '/discharge-form',\n        query: {\n          id: row.id,\n          mode: 'view'\n        }\n      });\n    },\n    printDischarge(row) {\n      this.selectedPatient = row;\n      this.showPrintModal = true;\n    },\n    closePrintModal() {\n      this.showPrintModal = false;\n    },\n    actualPrint() {\n      window.print();\n      this.closePrintModal();\n    }\n  },\n  computed: {\n    pendingDischargeCount() {\n      return this.tableData.filter(patient => patient.dischargeSummary.status === 'pending' || patient.dischargeHanding.status === 'pending').length;\n    }\n  },\n  mounted() {\n    this.fetchData();\n  }\n};","map":{"version":3,"names":["getDischargeList","name","data","loading","tableData","total","currentPage","pageSize","dateRange","searchQuery","activeTab","showPrintModal","selectedPatient","methods","fetchData","params","page","size","length","startDate","endDate","nameOrCode","response","code","content","totalElements","$message","error","message","console","handleDateChange","handleSearch","handleReset","handleSizeChange","val","handleCurrentChange","goToAssessment","$router","push","getStatusType","status","getOperationButton","row","dischargeSummary","dischargeHanding","type","icon","text","editDischarge","path","query","id","viewDischargeDetails","mode","printDischarge","closePrintModal","actualPrint","window","print","computed","pendingDischargeCount","filter","patient","mounted"],"sources":["src/views/Discharge.vue"],"sourcesContent":["<template>\n  <div class=\"assessment-container\">\n    <!-- 标签页导航 -->\n    <el-tabs v-model=\"activeTab\" type=\"card\" class=\"custom-tabs\">\n      <el-tab-pane name=\"assessment\">\n        <span slot=\"label\" @click=\"goToAssessment\">\n          入院评估\n          <el-badge :value=\"2\" class=\"tab-badge\" type=\"info\"></el-badge>\n        </span>\n      </el-tab-pane>\n      <el-tab-pane name=\"discharge\">\n        <span slot=\"label\">\n          离院办理\n          <el-badge :value=\"pendingDischargeCount\" class=\"tab-badge\" type=\"primary\"></el-badge>\n        </span>\n        <!-- 离院办理内容 -->\n        <div class=\"search-section\">\n          <div class=\"search-form\">\n            <div class=\"form-item\">\n              <span class=\"label\">离院日期</span>\n              <el-date-picker\n                v-model=\"dateRange\"\n                type=\"daterange\"\n                range-separator=\"~\"\n                start-placeholder=\"开始日期\"\n                end-placeholder=\"结束日期\"\n                format=\"yyyy/MM/dd\"\n                value-format=\"yyyy-MM-dd\"\n                :default-time=\"['00:00:00', '23:59:59']\"\n                @change=\"handleDateChange\">\n              </el-date-picker>\n            </div>\n            <div class=\"form-item\">\n              <el-input\n                v-model=\"searchQuery\"\n                placeholder=\"姓名/入院编号\"\n                class=\"search-input\">\n                <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"handleSearch\"></el-button>\n              </el-input>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"notice\">\n          <p>提示：完成离院流程手续后，离院小结数据不可再修改</p>\n        </div>\n\n        <!-- 表格内容 -->\n        <div class=\"content-section\">\n          <el-table\n            v-loading=\"loading\"\n            :data=\"tableData\"\n            style=\"width: 100%\"\n            border>\n            <el-table-column\n              label=\"姓名/身份证\"\n              width=\"200\">\n              <template slot-scope=\"scope\">\n                <div>{{ scope.row.name }}</div>\n                <div class=\"id-number\">{{ scope.row.idNumber }}</div>\n              </template>\n            </el-table-column>\n            <el-table-column\n              prop=\"gender\"\n              label=\"性别\"\n              width=\"80\">\n            </el-table-column>\n            <el-table-column\n              prop=\"age\"\n              label=\"年龄\"\n              width=\"80\">\n            </el-table-column>\n            <el-table-column\n              prop=\"admissionDate\"\n              label=\"入院日期\"\n              width=\"120\">\n            </el-table-column>\n            <el-table-column\n              prop=\"dischargeReason\"\n              label=\"离院原因\"\n              width=\"120\">\n            </el-table-column>\n            <el-table-column\n              prop=\"dischargeDate\"\n              label=\"离院日期\"\n              width=\"120\">\n            </el-table-column>\n            <el-table-column\n              label=\"离院小结\"\n              width=\"120\">\n              <template slot-scope=\"scope\">\n                <el-tag :type=\"getStatusType(scope.row.dischargeSummary.status)\" size=\"medium\">\n                  {{ scope.row.dischargeSummary.status === 'pending' ? '待录入' : '已完成' }}\n                </el-tag>\n              </template>\n            </el-table-column>\n            <el-table-column\n              label=\"离院交接\"\n              width=\"120\">\n              <template slot-scope=\"scope\">\n                <el-tag :type=\"getStatusType(scope.row.dischargeHanding.status)\" size=\"medium\">\n                  {{ scope.row.dischargeHanding.status === 'pending' ? '待交接' : '已完成' }}\n                </el-tag>\n              </template>\n            </el-table-column>\n            <el-table-column\n              label=\"操作\"\n              width=\"120\"\n              fixed=\"right\">\n              <template slot-scope=\"scope\">\n                <el-button\n                  type=\"text\"\n                  size=\"small\"\n                  @click=\"getOperationButton(scope.row).type === 'edit' ? editDischarge(scope.row) : viewDischargeDetails(scope.row)\">\n                  <i :class=\"getOperationButton(scope.row).icon\"></i>\n                  {{ getOperationButton(scope.row).text }}\n                </el-button>\n                <el-button\n                  v-if=\"scope.row.dischargeSummary.status === 'completed'\"\n                  type=\"text\"\n                  size=\"small\"\n                  @click=\"printDischarge(scope.row)\">\n                  <i class=\"el-icon-printer\"></i>\n                  打印\n                </el-button>\n              </template>\n            </el-table-column>\n          </el-table>\n\n          <!-- 分页 -->\n          <div class=\"pagination-section\">\n            <el-pagination\n              @size-change=\"handleSizeChange\"\n              @current-change=\"handleCurrentChange\"\n              :current-page=\"currentPage\"\n              :page-sizes=\"[10, 20, 50]\"\n              :page-size=\"pageSize\"\n              layout=\"total, sizes, prev, pager, next, jumper\"\n              :total=\"total\">\n            </el-pagination>\n          </div>\n        </div>\n      </el-tab-pane>\n    </el-tabs>\n\n    <!-- 打印模态框 -->\n    <el-dialog\n      title=\"离院小结\"\n      :visible.sync=\"showPrintModal\"\n      width=\"800px\"\n      class=\"print-dialog\"\n      :before-close=\"closePrintModal\">\n      <div class=\"print-content\">\n        <div class=\"print-header\">\n          <h2>上海XX机构</h2>\n          <h3>出院小结</h3>\n        </div>\n\n        <div class=\"patient-info\">\n          <div class=\"info-row\">\n            <div class=\"info-item\">\n              <label>姓名：</label>\n              <span>{{ selectedPatient.name }}</span>\n            </div>\n            <div class=\"info-item\">\n              <label>性别：</label>\n              <span>{{ selectedPatient.gender }}</span>\n            </div>\n            <div class=\"info-item\">\n              <label>年龄：</label>\n              <span>{{ selectedPatient.age }}</span>\n            </div>\n          </div>\n\n          <div class=\"info-row\">\n            <div class=\"info-item\">\n              <label>入院日期：</label>\n              <span>{{ selectedPatient.admissionDate }}</span>\n            </div>\n            <div class=\"info-item\">\n              <label>出院日期：</label>\n              <span>{{ selectedPatient.dischargeDate }}</span>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"section\">\n          <div class=\"section-title\">入院诊断</div>\n          <div class=\"section-content\">\n            <div>1. 高血压病</div>\n            <div>2. 美尼尔综合征</div>\n            <div>3. 老年性精神障碍</div>\n          </div>\n        </div>\n\n        <div class=\"section\">\n          <div class=\"section-title\">入院情况</div>\n          <div class=\"section-content\">\n            {{ selectedPatient.name }}，{{ selectedPatient.gender }}，{{ selectedPatient.age }}岁，因家中子女上班无人照料且生活无法自理入院。\n          </div>\n        </div>\n\n        <div class=\"section\">\n          <div class=\"section-title\">在院治疗情况</div>\n          <div class=\"section-content\">\n            入院后给予对症支持治疗，病情稳定。\n          </div>\n        </div>\n\n        <div class=\"section\">\n          <div class=\"section-title\">出院医嘱</div>\n          <div class=\"section-content\">\n            <div>1. 注意生活作息规律，清淡饮食</div>\n            <div>2. 控制血压</div>\n            <div>3. 定期检查，不适随诊</div>\n          </div>\n        </div>\n\n        <div class=\"doctor-info\">\n          <div>医生签名：________________</div>\n          <div>日期：{{ new Date().toLocaleDateString() }}</div>\n        </div>\n      </div>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"closePrintModal\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"actualPrint\">打 印</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { getDischargeList } from '../api/discharge'\n\nexport default {\n  name: 'DischargeManagement',\n  data() {\n    return {\n      loading: false,\n      tableData: [],\n      total: 0,\n      currentPage: 1,\n      pageSize: 10,\n      dateRange: [],\n      searchQuery: '',\n      activeTab: 'discharge',\n      showPrintModal: false,\n      selectedPatient: {}\n    }\n  },\n  methods: {\n    async fetchData() {\n      this.loading = true\n      try {\n        const params = {\n          page: this.currentPage - 1,\n          size: this.pageSize\n        }\n\n        if (this.dateRange && this.dateRange.length === 2) {\n          params.startDate = this.dateRange[0]\n          params.endDate = this.dateRange[1]\n        }\n\n        if (this.searchQuery) {\n          params.nameOrCode = this.searchQuery\n        }\n\n        const response = await getDischargeList(params)\n        \n        if (response.code === 200) {\n          this.tableData = response.data.content\n          this.total = response.data.totalElements\n        } else {\n          this.$message.error(response.message || '获取数据失败')\n        }\n      } catch (error) {\n        console.error('获取数据失败:', error)\n        this.$message.error('获取数据失败')\n      } finally {\n        this.loading = false\n      }\n    },\n    handleDateChange() {\n      this.currentPage = 1\n      this.fetchData()\n    },\n    handleSearch() {\n      this.currentPage = 1\n      this.fetchData()\n    },\n    handleReset() {\n      this.dateRange = []\n      this.searchQuery = ''\n      this.currentPage = 1\n      this.fetchData()\n    },\n    handleSizeChange(val) {\n      this.pageSize = val\n      this.fetchData()\n    },\n    handleCurrentChange(val) {\n      this.currentPage = val\n      this.fetchData()\n    },\n    goToAssessment() {\n      this.$router.push('/ass-assessment')\n    },\n    getStatusType(status) {\n      return status === 'pending' ? 'warning' : 'success'\n    },\n    getOperationButton(row) {\n      if (row.dischargeSummary.status === 'completed' && row.dischargeHanding.status === 'completed') {\n        return {\n          type: 'view',\n          icon: 'el-icon-view',\n          text: '查看'\n        }\n      } else {\n        return {\n          type: 'edit',\n          icon: 'el-icon-edit',\n          text: '编辑'\n        }\n      }\n    },\n    editDischarge(row) {\n      this.$router.push({\n        path: '/discharge-form',\n        query: { id: row.id }\n      })\n    },\n    viewDischargeDetails(row) {\n      this.$router.push({\n        path: '/discharge-form',\n        query: { id: row.id, mode: 'view' }\n      })\n    },\n    printDischarge(row) {\n      this.selectedPatient = row\n      this.showPrintModal = true\n    },\n    closePrintModal() {\n      this.showPrintModal = false\n    },\n    actualPrint() {\n      window.print()\n      this.closePrintModal()\n    }\n  },\n  computed: {\n    pendingDischargeCount() {\n      return this.tableData.filter(patient => \n        patient.dischargeSummary.status === 'pending' || \n        patient.dischargeHanding.status === 'pending'\n      ).length\n    }\n  },\n  mounted() {\n    this.fetchData()\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.assessment-container {\n  background: #f0f4f7;\n  border-radius: 8px;\n  padding: 16px;\n  box-shadow: 0 1px 6px 0 rgba(0, 0, 0, 0.05);\n\n  .custom-tabs {\n    /deep/ .el-tabs__header {\n      margin-bottom: 16px;\n      border-bottom: none;\n    }\n\n    /deep/ .el-tabs__item {\n      height: 40px;\n      line-height: 40px;\n      font-size: 14px;\n      padding: 0 20px;\n      transition: all 0.3s;\n      \n      &.is-active {\n        color: #3a7d7c;\n        font-weight: 600;\n        background: rgba(58, 125, 124, 0.1);\n        border-radius: 4px;\n      }\n\n      &:hover {\n        color: #3a7d7c;\n      }\n    }\n  }\n\n  .tab-badge {\n    margin-left: 6px;\n    \n    /deep/ .el-badge__content {\n      height: 18px;\n      line-height: 18px;\n      padding: 0 6px;\n      border: none;\n      border-radius: 9px;\n      font-weight: 600;\n      font-size: 11px;\n      background-color: #3a7d7c;\n      color: #fff;\n    }\n  }\n\n  .search-section {\n    margin-bottom: 16px;\n    background: #e8ecef;\n    padding: 16px;\n    border-radius: 6px;\n    \n    .search-form {\n      display: flex;\n      align-items: center;\n      gap: 16px;\n\n      .form-item {\n        display: flex;\n        align-items: center;\n\n        .label {\n          margin-right: 10px;\n          color: #5a6e6c;\n          font-weight: 500;\n        }\n\n        .search-input {\n          width: 200px;\n\n          /deep/ .el-input__inner {\n            border-radius: 4px;\n            height: 34px;\n            line-height: 34px;\n            border-color: #b0c4c3;\n          }\n\n          /deep/ .el-input-group__append {\n            background: #3a7d7c;\n            border-color: #3a7d7c;\n            color: #fff;\n            padding: 0 12px;\n            \n            &:hover {\n              background: #5a9d9c;\n              border-color: #5a9d9c;\n            }\n          }\n        }\n      }\n    }\n  }\n\n  .notice {\n    padding: 10px 16px;\n    background: rgba(58, 125, 124, 0.1);\n    border-radius: 4px;\n    margin-bottom: 16px;\n\n    p {\n      color: #3a7d7c;\n      font-size: 13px;\n      margin: 0;\n    }\n  }\n\n  .content-section {\n    background: #fff;\n    border-radius: 6px;\n    padding: 16px;\n    min-height: 400px;\n\n    /deep/ .el-table {\n      font-size: 13px;\n      color: #3a4a49;\n      \n      th {\n        background-color: #e8ecef;\n        color: #3a4a49;\n        font-weight: 600;\n        padding: 12px 0;\n      }\n\n      td {\n        padding: 12px;\n      }\n\n      .el-table__row:hover {\n        background-color: #f5f7f8;\n      }\n    }\n\n    .id-number {\n      font-size: 12px;\n      color: #909399;\n      margin-top: 4px;\n    }\n\n    .el-tag {\n      border-radius: 4px;\n      padding: 0 12px;\n      height: 24px;\n      line-height: 24px;\n    }\n\n    .el-button--text {\n      color: #3a7d7c;\n      padding: 0 8px;\n      \n      &:hover {\n        color: #5a9d9c;\n      }\n\n      i {\n        margin-right: 4px;\n      }\n    }\n  }\n\n  .pagination-section {\n    margin-top: 16px;\n    display: flex;\n    justify-content: flex-end;\n  }\n}\n\n.print-dialog {\n  /deep/ .el-dialog__header {\n    padding: 20px;\n    border-bottom: 1px solid #f0f2f5;\n  }\n\n  .print-content {\n    padding: 20px;\n\n    .print-header {\n      text-align: center;\n      margin-bottom: 24px;\n\n      h2 {\n        font-size: 20px;\n        margin-bottom: 8px;\n      }\n\n      h3 {\n        font-size: 16px;\n        font-weight: normal;\n        color: #5a6e6c;\n      }\n    }\n\n    .patient-info {\n      margin-bottom: 24px;\n      \n      .info-row {\n        display: flex;\n        margin-bottom: 16px;\n        \n        .info-item {\n          flex: 1;\n          display: flex;\n          align-items: center;\n          \n          label {\n            color: #909399;\n            width: 80px;\n          }\n        }\n      }\n    }\n\n    .section {\n      margin-bottom: 24px;\n\n      .section-title {\n        font-size: 15px;\n        color: #3a4a49;\n        font-weight: 600;\n        margin-bottom: 12px;\n        padding-left: 12px;\n        border-left: 3px solid #3a7d7c;\n      }\n\n      .section-content {\n        color: #5a6e6c;\n        line-height: 1.8;\n        padding: 0 12px;\n\n        div {\n          margin-bottom: 8px;\n        }\n      }\n    }\n\n    .doctor-info {\n      display: flex;\n      justify-content: flex-end;\n      gap: 24px;\n      margin-top: 32px;\n      color: #5a6e6c;\n    }\n  }\n}\n\n@media print {\n  .assessment-container {\n    background: none;\n    padding: 0;\n    box-shadow: none;\n  }\n\n  .custom-tabs,\n  .search-section,\n  .notice,\n  .pagination-section,\n  .el-dialog__header,\n  .el-dialog__footer {\n    display: none !important;\n  }\n\n  .print-content {\n    padding: 0 !important;\n  }\n}\n</style> "],"mappings":";;;AAwOA,SAAAA,gBAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,SAAA;MACAC,KAAA;MACAC,WAAA;MACAC,QAAA;MACAC,SAAA;MACAC,WAAA;MACAC,SAAA;MACAC,cAAA;MACAC,eAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,UAAA;MACA,KAAAX,OAAA;MACA;QACA,MAAAY,MAAA;UACAC,IAAA,OAAAV,WAAA;UACAW,IAAA,OAAAV;QACA;QAEA,SAAAC,SAAA,SAAAA,SAAA,CAAAU,MAAA;UACAH,MAAA,CAAAI,SAAA,QAAAX,SAAA;UACAO,MAAA,CAAAK,OAAA,QAAAZ,SAAA;QACA;QAEA,SAAAC,WAAA;UACAM,MAAA,CAAAM,UAAA,QAAAZ,WAAA;QACA;QAEA,MAAAa,QAAA,SAAAtB,gBAAA,CAAAe,MAAA;QAEA,IAAAO,QAAA,CAAAC,IAAA;UACA,KAAAnB,SAAA,GAAAkB,QAAA,CAAApB,IAAA,CAAAsB,OAAA;UACA,KAAAnB,KAAA,GAAAiB,QAAA,CAAApB,IAAA,CAAAuB,aAAA;QACA;UACA,KAAAC,QAAA,CAAAC,KAAA,CAAAL,QAAA,CAAAM,OAAA;QACA;MACA,SAAAD,KAAA;QACAE,OAAA,CAAAF,KAAA,YAAAA,KAAA;QACA,KAAAD,QAAA,CAAAC,KAAA;MACA;QACA,KAAAxB,OAAA;MACA;IACA;IACA2B,iBAAA;MACA,KAAAxB,WAAA;MACA,KAAAQ,SAAA;IACA;IACAiB,aAAA;MACA,KAAAzB,WAAA;MACA,KAAAQ,SAAA;IACA;IACAkB,YAAA;MACA,KAAAxB,SAAA;MACA,KAAAC,WAAA;MACA,KAAAH,WAAA;MACA,KAAAQ,SAAA;IACA;IACAmB,iBAAAC,GAAA;MACA,KAAA3B,QAAA,GAAA2B,GAAA;MACA,KAAApB,SAAA;IACA;IACAqB,oBAAAD,GAAA;MACA,KAAA5B,WAAA,GAAA4B,GAAA;MACA,KAAApB,SAAA;IACA;IACAsB,eAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACAC,cAAAC,MAAA;MACA,OAAAA,MAAA;IACA;IACAC,mBAAAC,GAAA;MACA,IAAAA,GAAA,CAAAC,gBAAA,CAAAH,MAAA,oBAAAE,GAAA,CAAAE,gBAAA,CAAAJ,MAAA;QACA;UACAK,IAAA;UACAC,IAAA;UACAC,IAAA;QACA;MACA;QACA;UACAF,IAAA;UACAC,IAAA;UACAC,IAAA;QACA;MACA;IACA;IACAC,cAAAN,GAAA;MACA,KAAAL,OAAA,CAAAC,IAAA;QACAW,IAAA;QACAC,KAAA;UAAAC,EAAA,EAAAT,GAAA,CAAAS;QAAA;MACA;IACA;IACAC,qBAAAV,GAAA;MACA,KAAAL,OAAA,CAAAC,IAAA;QACAW,IAAA;QACAC,KAA