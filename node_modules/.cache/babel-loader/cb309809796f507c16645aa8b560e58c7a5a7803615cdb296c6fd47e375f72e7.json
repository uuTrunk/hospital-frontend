{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { getAdmissionList } from '@/api/admission';\nexport default {\n  name: 'PatientAssessment',\n  data() {\n    return {\n      activeTab: 'assessment',\n      dateRange: [],\n      searchText: '',\n      currentPage: 1,\n      pageSize: 10,\n      loading: false,\n      patientList: [],\n      total: 0\n    };\n  },\n  computed: {\n    filteredPatients() {\n      if (!this.searchText && !this.dateRange.length) {\n        return this.patientList;\n      }\n      return this.patientList.filter(patient => {\n        let matchesSearch = true;\n        let matchesDate = true;\n\n        // 搜索文本过滤\n        if (this.searchText) {\n          const searchLower = this.searchText.toLowerCase();\n          matchesSearch = patient.name.toLowerCase().includes(searchLower) || patient.admissionNo?.toLowerCase().includes(searchLower);\n        }\n\n        // 日期范围过滤\n        if (this.dateRange && this.dateRange.length === 2) {\n          const registrationDate = new Date(patient.registrationTime);\n          const startDate = new Date(this.dateRange[0]);\n          const endDate = new Date(this.dateRange[1]);\n          matchesDate = registrationDate >= startDate && registrationDate <= endDate;\n        }\n        return matchesSearch && matchesDate;\n      });\n    },\n    displayPatients() {\n      return this.patientList;\n    }\n  },\n  created() {\n    this.fetchPatientList();\n  },\n  methods: {\n    async fetchPatientList() {\n      try {\n        this.loading = true;\n        const params = {\n          page: this.currentPage,\n          size: this.pageSize,\n          keyword: this.searchText,\n          startDate: this.dateRange?.[0],\n          endDate: this.dateRange?.[1]\n        };\n        const response = await getAdmissionList(params);\n        if (response.data) {\n          this.patientList = response.data.records || [];\n          this.total = response.data.total || 0;\n        }\n      } catch (error) {\n        console.error('获取患者列表失败:', error);\n        this.$message.error('获取患者列表失败，请稍后重试');\n      } finally {\n        this.loading = false;\n      }\n    },\n    handleDateChange(val) {\n      this.dateRange = val;\n      this.currentPage = 1;\n      this.fetchPatientList();\n    },\n    handleSizeChange(val) {\n      this.pageSize = val;\n      this.currentPage = 1;\n      this.fetchPatientList();\n    },\n    handleCurrentChange(val) {\n      this.currentPage = val;\n      this.fetchPatientList();\n    },\n    formatDate(date) {\n      if (!date) return '';\n      const d = new Date(date);\n      return `${d.getFullYear()}-${String(d.getMonth() + 1).padStart(2, '0')}-${String(d.getDate()).padStart(2, '0')} ${String(d.getHours()).padStart(2, '0')}:${String(d.getMinutes()).padStart(2, '0')}`;\n    },\n    canEdit(row) {\n      return row.healthStatus === '待评估' || row.healthStatus === '未完成';\n    },\n    editAssessment(row) {\n      this.$router.push(`/health-assessment/${row.patientId}`);\n    },\n    viewDetails(row) {\n      this.$router.push({\n        path: '/admission-details',\n        query: {\n          id: row.patientId\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["getAdmissionList","name","data","activeTab","dateRange","searchText","currentPage","pageSize","loading","patientList","total","computed","filteredPatients","length","filter","patient","matchesSearch","matchesDate","searchLower","toLowerCase","includes","admissionNo","registrationDate","Date","registrationTime","startDate","endDate","displayPatients","created","fetchPatientList","methods","params","page","size","keyword","response","records","error","console","$message","handleDateChange","val","handleSizeChange","handleCurrentChange","formatDate","date","d","getFullYear","String","getMonth","padStart","getDate","getHours","getMinutes","canEdit","row","hea