{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: 'DischargeView',\n  data() {\n    return {\n      startDate: '',\n      endDate: '',\n      searchKeyword: '',\n      currentPage: 1,\n      pageSize: 10,\n      totalPages: 9,\n      patients: [{\n        id: 1,\n        name: 'ÊùéÂ§ßÂì•',\n        idNumber: '310106194805162428',\n        gender: 'Áî∑',\n        age: 78,\n        admissionDate: '2024-05-22',\n        dischargeReason: 'Ëá™Âä®Âá∫Èô¢',\n        dischargeDate: '2025-01-22',\n        dischargeSummary: {\n          status: 'pending',\n          time: '',\n          operator: ''\n        },\n        dischargeHanding: {\n          status: 'completed',\n          time: '',\n          operator: 'Áéã‰∏ª‰ªª'\n        }\n      }, {\n        id: 2,\n        name: 'ÁéãÂ§ßÂ®ò',\n        idNumber: '310106194805162428',\n        gender: 'Â•≥',\n        age: 98,\n        admissionDate: '2023-05-18',\n        dischargeReason: 'Êú¨Èô¢Ê≠ª‰∫°',\n        dischargeDate: '2024-01-18',\n        dischargeSummary: {\n          status: 'completed',\n          time: '2024-05-18 12:18',\n          operator: 'ÂàòÂåªÁîü'\n        },\n        dischargeHanding: {\n          status: 'completed',\n          time: '',\n          operator: 'Áéã‰∏ª‰ªª'\n        }\n      }, {\n        id: 3,\n        name: 'ÂÆãËÄÅÂçö',\n        idNumber: '310106194805162428',\n        gender: 'Áî∑',\n        age: 89,\n        admissionDate: '2024-05-12',\n        dischargeReason: 'Â§ñÈô¢Ê≠ª‰∫°',\n        dischargeDate: '2024-09-12',\n        dischargeSummary: {\n          status: 'completed',\n          time: '2024-05-18 12:18',\n          operator: 'ÂàòÂåªÁîü'\n        },\n        dischargeHanding: {\n          status: 'completed',\n          time: '2024-05-18 12:18',\n          operator: 'Âº†Êä§Â£´'\n        }\n      }]\n    };\n  },\n  computed: {\n    visiblePages() {\n      const pages = [];\n      let startPage = Math.max(this.currentPage - 2, 1);\n      let endPage = Math.min(startPage + 4, this.totalPages);\n      if (endPage - startPage < 4) {\n        startPage = Math.max(endPage - 4, 1);\n      }\n      for (let i = startPage; i <= endPage; i++) {\n        pages.push(i);\n      }\n      return pages;\n    },\n    filteredPatients() {\n      // ËøáÊª§ÊÇ£ËÄÖÂàóË°®\n      let result = [...this.patients];\n\n      // Ê†πÊçÆÂÖ≥ÈîÆÂ≠óÊêúÁ¥¢\n      if (this.searchKeyword) {\n        const keyword = this.searchKeyword.toLowerCase();\n        result = result.filter(patient => patient.name.toLowerCase().includes(keyword) || patient.idNumber.includes(keyword));\n      }\n\n      // Ê†πÊçÆÊó•ÊúüËåÉÂõ¥Á≠õÈÄâ\n      if (this.startDate && this.endDate) {\n        const start = new Date(this.startDate);\n        const end = new Date(this.endDate);\n        result = result.filter(patient => {\n          const dischargeDate = new Date(patient.dischargeDate);\n          return dischargeDate >= start && dischargeDate <= end;\n        });\n      }\n      return result;\n    }\n  },\n  methods: {\n    goToAdmission() {\n      this.$router.push('/admission-assessment');\n    },\n    searchPatients() {\n      // ÂÆûÁé∞ÊêúÁ¥¢ÂäüËÉΩ\n      console.log('ÊêúÁ¥¢:', this.searchKeyword, this.startDate, this.endDate);\n      this.currentPage = 1; // ÈáçÁΩÆÂà∞Á¨¨‰∏ÄÈ°µ\n    },\n    changePage(page) {\n      if (page >= 1 && page <= this.totalPages) {\n        this.currentPage = page;\n      }\n    },\n    changePageSize() {\n      this.currentPage = 1; // ÂàáÊç¢ÊØèÈ°µÊòæÁ§∫Êï∞ÈáèÊó∂ÈáçÁΩÆÂà∞Á¨¨‰∏ÄÈ°µ\n    },\n    editDischarge(patient) {\n      // ÁºñËæëÁ¶ªÈô¢‰ø°ÊÅØ\n      console.log('ÁºñËæëÁ¶ªÈô¢‰ø°ÊÅØ:', patient);\n      // ËøôÈáåÂèØ‰ª•ÂÆûÁé∞ÁºñËæëÈÄªËæëÔºå‰æãÂ¶ÇÂºπÂá∫Ë°®ÂçïÊàñË∑≥ËΩ¨Âà∞ÁºñËæëÈ°µÈù¢\n\n      // Â¶ÇÊûúÊòØÂæÖÂΩïÂÖ•ÁöÑÂ∞èÁªìÔºåÂèØ‰ª•ËøõË°åÂΩïÂÖ•Êìç‰ΩúÂπ∂‰øùÂ≠ò\n      if (patient.dischargeSummary.status === 'pending') {\n        // Ê®°Êãü‰øùÂ≠òÈÄªËæë\n        const now = new Date();\n        const timeStr = `${now.getFullYear()}-${String(now.getMonth() + 1).padStart(2, '0')}-${String(now.getDate()).padStart(2, '0')} ${String(now.getHours()).padStart(2, '0')}:${String(now.getMinutes()).padStart(2, '0')}`;\n\n        // Êõ¥Êñ∞Áä∂ÊÄÅ‰∏∫Â∑≤ÂÆåÊàê\n        patient.dischargeSummary.status = 'completed';\n        patient.dischargeSummary.time = timeStr;\n        patient.dischargeSummary.operator = 'ÂΩìÂâçÁî®Êà∑'; // Âú®ÂÆûÈôÖÈ°πÁõÆ‰∏≠ÔºåËøôÈáåÂ∫îËØ•ÊòØÂΩìÂâçÁôªÂΩïÁî®Êà∑ÁöÑÂêçÁß∞\n      }\n    },\n    printDischarge(patient) {\n      // ÊâìÂç∞Á¶ªÈô¢‰ø°ÊÅØ\n      console.log('ÊâìÂç∞Á¶ªÈô¢‰ø°ÊÅØ:', patient);\n    },\n    viewDischargeDetails(patient) {\n      // Êü•ÁúãÁ¶ªÈô¢ËØ¶ÊÉÖ\n      console.log('Êü•ÁúãÁ¶ªÈô¢ËØ¶ÊÉÖ:', patient);\n    },\n    canEdit(patient) {\n      // Âà§Êñ≠ÊòØÂê¶ÂèØ‰ª•ÁºñËæëÁ¶ªÈô¢‰ø°ÊÅØ\n      // ËøôÈáåÂèØ‰ª•Ê∑ªÂä†Êõ¥Â§öÈÄªËæëÂà§Êñ≠Ôºå‰æãÂ¶ÇÊ†πÊçÆÊùÉÈôêÊàñÁä∂ÊÄÅÂÜ≥ÂÆö\n      return patient.id !== 3; // Á§∫‰æãÈÄªËæëÔºåÂÆûÈôÖÂ∫îÊ†πÊçÆ‰∏öÂä°ÈúÄÊ±ÇÂÆûÁé∞\n    }\n  }\n};","map":{"version":3,"names":["name","data","startDate","endDate","searchKeyword","currentPage","pageSize","totalPages","patients","id","idNumber","gender","age","admissionDate","dischargeReason","dischargeDate","dischargeSummary","status","time","operator","dischargeHanding","computed","visiblePages","pages","startPage","Math","max","endPage","min","i","push","filteredPatients","result","keyword","toLowerCase","filter","patient","includes","start","Date","end","methods","goToAdmission","$router","searchPatients","console","log","changePage","page","changePageSize","editDischarge","now","timeStr","getFullYear","String","getMonth","padStart","getDate","getHours","getMinutes","printDischarge","viewDischargeDetails","canEdit"],"sources":["src/views/Discharge.vue"],"sourcesContent":["<template>\r\n  <div class=\"discharge\">\r\n    <!-- ÈÄâÈ°πÂç°Ê†áÈ¢ò -->\r\n    <div class=\"tabs\">\r\n      <div class=\"tab\" :class=\"{ active: false }\" @click=\"goToAdmission\">ÂÖ•Èô¢ËØÑ‰º∞(2)</div>\r\n      <div class=\"tab\" :class=\"{ active: true }\">Á¶ªÈô¢ÂäûÁêÜ(1)</div>\r\n      <div class=\"tab-active-indicator\" :style=\"{ left: '143px' }\"></div>\r\n    </div>\r\n\r\n    <!-- ÊêúÁ¥¢ÂíåÁ≠õÈÄâÂå∫Âüü -->\r\n    <div class=\"filter-area\">\r\n      <div class=\"date-filter\">\r\n        <span>Á¶ªÈô¢Êó•Êúü</span>\r\n        <input type=\"date\" class=\"date-input\" v-model=\"startDate\">\r\n        <span>Ëá≥</span>\r\n        <input type=\"date\" class=\"date-input\" v-model=\"endDate\">\r\n        <button class=\"search-btn\" @click=\"searchPatients\"><i class=\"icon-search\"></i></button>\r\n      </div>\r\n      <div class=\"search-box\">\r\n        <input type=\"text\" class=\"search-input\" placeholder=\"ÂßìÂêç/ÂÖ•Èô¢ÁºñÂè∑\" v-model=\"searchKeyword\">\r\n        <button class=\"search-btn\" @click=\"searchPatients\"><i class=\"icon-search\"></i></button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ÊèêÁ§∫‰ø°ÊÅØ -->\r\n    <div class=\"notice\">\r\n      <p>ÊèêÁ§∫ÔºöÂÆåÂÖ®Á¶ªÈô¢ÊµÅÁ®ãÊâãÁª≠ÂêéÔºåÁ¶ªÈô¢Â∞èÁªìÊï∞ÊçÆ‰∏çÂèØÂÜç‰øÆÊîπ</p>\r\n    </div>\r\n\r\n    <!-- Ë°®Ê†ºÂÜÖÂÆπ -->\r\n    <div class=\"table-wrapper\">\r\n      <table class=\"data-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>ÂßìÂêç/Ë∫´‰ªΩËØÅ</th>\r\n            <th>ÊÄßÂà´</th>\r\n            <th>Âπ¥ÈæÑ</th>\r\n            <th>ÂÖ•Èô¢Êó•Êúü</th>\r\n            <th>Á¶ªÈô¢ÂéüÂõ†</th>\r\n            <th>Á¶ªÈô¢Êó•Êúü</th>\r\n            <th>Á¶ªÈô¢Â∞èÁªì</th>\r\n            <th>Á¶ªÈô¢‰∫§Êé•</th>\r\n            <th>Êìç‰Ωú</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"patient in filteredPatients\" :key=\"patient.id\">\r\n            <td>\r\n              <div>{{ patient.name }}</div>\r\n              <div class=\"id-number\">{{ patient.idNumber }}</div>\r\n            </td>\r\n            <td>{{ patient.gender }}</td>\r\n            <td>{{ patient.age }}</td>\r\n            <td>{{ patient.admissionDate }}</td>\r\n            <td>{{ patient.dischargeReason }}</td>\r\n            <td>{{ patient.dischargeDate }}</td>\r\n            <td class=\"status-cell\">\r\n              <div v-if=\"patient.dischargeSummary.status === 'pending'\" class=\"status-pending\">\r\n                ÂæÖÂΩïÂÖ•\r\n              </div>\r\n              <div v-else-if=\"patient.dischargeSummary.status === 'completed'\" class=\"status-completed\" :title=\"'ÁªèÂäû‰∫∫Ôºö' + (patient.dischargeSummary.operator || 'Êú™Áü•')\">\r\n                <i class=\"icon-check\"></i>\r\n                <div>{{ patient.dischargeSummary.time }}</div>\r\n              </div>\r\n            </td>\r\n            <td class=\"status-cell\">\r\n              <div v-if=\"patient.dischargeHanding.status === 'pending'\" class=\"status-pending\">\r\n                ÂæÖ‰∫§Êé•\r\n              </div>\r\n              <div v-else-if=\"patient.dischargeHanding.status === 'completed'\" class=\"status-completed\" :title=\"'ÁªèÂäû‰∫∫Ôºö' + (patient.dischargeHanding.operator || 'Êú™Áü•')\">\r\n                <i class=\"icon-check\"></i>\r\n                <div>{{ patient.dischargeHanding.time || 'ÁâπÂÆåÊàê' }}</div>\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div v-if=\"patient.dischargeSummary.status === 'pending'\" class=\"action-btns\">\r\n                <button class=\"edit-btn\" @click=\"editDischarge(patient)\"><i class=\"icon-edit\"></i></button>\r\n              </div>\r\n              <div v-else class=\"action-btns\">\r\n                <button class=\"print-btn\" @click=\"printDischarge(patient)\"><i class=\"icon-print\"></i></button>\r\n                <button v-if=\"canEdit(patient)\" class=\"edit-btn\" @click=\"editDischarge(patient)\"><i class=\"icon-edit\"></i></button>\r\n                <button v-else class=\"detail-btn\" @click=\"viewDischargeDetails(patient)\"><i class=\"icon-detail\"></i></button>\r\n              </div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n\r\n    <!-- ÂàÜÈ°µÂå∫Âüü -->\r\n    <div class=\"pagination\">\r\n      <button class=\"page-btn prev\" :disabled=\"currentPage === 1\" @click=\"changePage(currentPage - 1)\"><i class=\"icon-prev\"></i></button>\r\n      <div class=\"page-numbers\">\r\n        <span \r\n          v-for=\"page in visiblePages\" \r\n          :key=\"page\" \r\n          class=\"page-number\" \r\n          :class=\"{ active: currentPage === page }\" \r\n          @click=\"changePage(page)\"\r\n        >\r\n          {{ page }}\r\n        </span>\r\n      </div>\r\n      <button class=\"page-btn next\" :disabled=\"currentPage === totalPages\" @click=\"changePage(currentPage + 1)\"><i class=\"icon-next\"></i></button>\r\n      <div class=\"page-size-selector\">\r\n        <select v-model=\"pageSize\" @change=\"changePageSize\">\r\n          <option value=\"10\">10Êù°/È°µ</option>\r\n          <option value=\"20\">20Êù°/È°µ</option>\r\n          <option value=\"50\">50Êù°/È°µ</option>\r\n        </select>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ÊèêÁ§∫Èù¢Êùø -->\r\n    <div class=\"tip-panel\">\r\n      <ol>\r\n        <li>ÂÖªËÄÅÈô¢Êñ∞Áî≥ËØ∑ËÄÖÔºåËøõÂÖ•ËØÑ‰º∞ÔºàÊú™Áî≥ËØ∑ÈúÄÊ±ÇÁöÑËÄÅ‰∫∫Ôºå‰∏çÂú®Ê≠§ÂàóË°®Â±ïÁ§∫Ôºâ</li>\r\n        <li>Êú™ÂÆåÊàêÂÅ•Â∫∑ËØÑ‰º∞ÂâçÔºåÂùáÂ§Ñ‰∏∫ÂæÖËØÑ‰º∞</li>\r\n        <li>ÂÆåÂÖ®ÊµÅÁ®ãÁî≥ËØ∑ÊµÅÁ®ãÔºàÁªøËâ≤Â∞èÁÇπÔºâÔºåÊèêÁ§∫ËØÑ‰º∞ÂÖ•Èô¢ÂΩïÔºàÂÖ•Èô¢Ê∞ëËé∑ÂæóÂèØÂú®Áä∂ÊÄÅÔºâ</li>\r\n      </ol>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'DischargeView',\r\n  data() {\r\n    return {\r\n      startDate: '',\r\n      endDate: '',\r\n      searchKeyword: '',\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      totalPages: 9,\r\n      patients: [\r\n        {\r\n          id: 1,\r\n          name: 'ÊùéÂ§ßÂì•',\r\n          idNumber: '310106194805162428',\r\n          gender: 'Áî∑',\r\n          age: 78,\r\n          admissionDate: '2024-