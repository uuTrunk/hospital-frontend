{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  methods: {\n    logout() {\n      window.sessionStorage.clear();\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["methods","logout","window","sessionStorage","clear","$router","push"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <el-container class=\"home-container\">\n    <!-- 侧边栏 -->\n    <el-aside width=\"200px\" class=\"sidebar\">\n      <div class=\"logo-container\">\n        <img src=\"../assets/logo3.png\" alt=\"logo3\">\n        <span>医护管理系统</span>\n      </div>\n      <el-menu router=\"true\"\n        background-color=\"#001529\"\n        text-color=\"rgba(255,255,255,0.65)\"\n        active-text-color=\"#ffd04b\"\n        style=\"border:none\">\n        <!-- 一级菜单 -->\n        <el-submenu index=\"1\">\n          <!-- 一级菜单的模板区域-->\n          <template slot=\"title\">\n            <!-- 图标 -->\n            <i class=\"el-icon-s-check\"></i>\n            <!-- 文本 -->\n            <span>离入院管理</span>\n          </template>\n\n          <!-- 二级菜单 -->\n          <el-menu-item index=\"/assessment\">\n            <template slot=\"title\">\n              <!-- 图标 -->\n              <i class=\"el-icon-s-claim\"></i>\n              <span>入院评估</span>\n            </template>\n          </el-menu-item>\n\n          <el-menu-item index=\"/discharge\">\n            <template slot=\"title\">\n              <!-- 图标 -->\n              <i class=\"el-icon-s-release\"></i>\n              <span>离院办理</span>\n            </template>\n          </el-menu-item>\n        </el-submenu>\n\n        <el-menu-item index=\"2\">\n          <i class=\"el-icon-files\"></i>\n          <span slot=\"title\">健康档案</span>\n        </el-menu-item>\n\n        <el-menu-item index=\"3\">\n          <i class=\"el-icon-document\"></i>\n          <span slot=\"title\">查房管理</span>\n        </el-menu-item>\n        <el-menu-item index=\"4\">\n          <i class=\"el-icon-